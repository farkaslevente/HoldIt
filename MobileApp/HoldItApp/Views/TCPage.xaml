<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HoldItApp.Views.TCPage"
             Title="TCPage"
             Shell.NavBarIsVisible="False">
    <Grid RowDefinitions="*" Margin="0,20,0,0" HorizontalOptions="FillAndExpand">
        
        <ScrollView Grid.Row="0" Margin="-30,-110,-30,0">
            <Image Source="tc.png"/>
        </ScrollView>
        <ImageButton Source="back.svg" HeightRequest="30" WidthRequest="40" Clicked="ImageButton_Clicked" 
             Grid.Row="0" 
             HorizontalOptions="StartAndExpand"
                     VerticalOptions="Start"/>
    </Grid>
</ContentPage>